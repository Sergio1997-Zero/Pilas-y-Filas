class ejerci
{
    constructor()
    {
        this.elementos={}   
        this.pares = {}    
        this.principio1=0
        this.final01=0  
        this.final02 = 0      
    }

    encolar(tamano)
    {
        for(let i=0; i < tamano; i++)
        {
            let valor=prompt("ingrese un valor: ")
            this.elementos[this.final01]=valor
            this.final01++
        }
    }

    Pares()
    {
        for(let i = 0; i < this.final01; i++)
        {
            let valor = parseInt(this.elementos[i])
            if(valor % 2 == 0)
            {
                this.pares[this.final02] = valor
                this.final02++                
            }
        }
        console.log(this.pares)
    }
}
let ejer1 = new ejerci()
ejer1.encolar(5)
ejer1.Pares()